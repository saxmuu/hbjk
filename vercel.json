{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/bypass",
      "dest": "/api/bypass.js",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/api/eu",
      "dest": "/api/eu-bypass.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/us",
      "dest": "/api/us-bypass.js", 
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/asia",
      "dest": "/api/asia-bypass.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "regions": ["all"],
  "env": {
    "NODE_ENV": "production"
  }
}