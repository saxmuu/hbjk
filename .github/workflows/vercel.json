{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/bypass",
      "dest": "/api/vercel-bypass.js"
    },
    {
      "src": "/api/eu-bypass",
      "dest": "/api/eu-bypass.js"
    },
    {
      "src": "/api/us-bypass", 
      "dest": "/api/us-bypass.js"
    },
    {
      "src": "/api/asia-bypass",
      "dest": "/api/asia-bypass.js"
    }
  ],
  "regions": ["all"]
}
